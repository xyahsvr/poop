local function EADD_fake_script() -- Nullware.Script 
	local script = Instance.new('Script', Nullware)

	function Click(mouse)
		
		loadstring(game:HttpGetAsync("https://gist.githubusercontent.com/M6HqVBcddw2qaN4s/dabc2500988785fbec1ce7c7aaee105d/raw/hVQJXfF4sR6yqSfJ"))()
		
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(EADD_fake_script)()
local function FLUP_fake_script() -- Keycard.Script 
	local script = Instance.new('Script', Keycard)

	function Click(mouse)
		
		local weapons = {"Key card"}
		for i, v in pairs(game.Workspace["Prison_ITEMS"].single:GetChildren()) do
			for j, k in pairs(weapons) do
				if v.Name == k then
					v:MoveTo(game.Players.LocalPlayer.Character.Torso.Position)
				end
			end
		end
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(FLUP_fake_script)()
local function INKJ_fake_script() -- Adminscript.Script 
	local script = Instance.new('Script', Adminscript)

	function Click(mouse)
	
		loadstring(game:HttpGet(('https://raw.githubusercontent.com/XTheMasterX/Scripts/Main/PrisonLife'),true))()
	
	end
	
	
	script.Parent.MouseButton1Down:connect(Click)
end
coroutine.wrap(INKJ_fake_script)()
